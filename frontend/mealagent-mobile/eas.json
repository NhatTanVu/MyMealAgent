{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://mymealagent-api.azurewebsites.net",
        "EXPO_PUBLIC_API_TIMEOUT": "60000"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
